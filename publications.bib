{% for paper in site.data.publications %}
{% if paper.type == "InProceedings" or paper.type == "Conference" or paper.type == "Workshop" %}
@inproceedings{ {{paper.id}}{% elsif paper.type == "Proceedings" %}
@proceedings{ {{paper.id}}{% elsif paper.type == "Book" %}
@book{ {{paper.id}}{% elsif paper.type == "Article" %}
@article{ {{paper.id}}{% elsif paper.type == "InCollection"%}
@incollection{ {{paper.id}}{% elsif paper.type == "PhDThesis" %}
@phdthesis{ {{paper.id}}{% elsif paper.type == "Misc"%}
@misc{ {{paper.id}}{% elsif paper.type == "Patent" %}
@misc{ {{paper.id}}{% endif %}{% if paper.title %},
    title="{{paper.title}}"{% endif %}{% if paper.address %},
    address="{{paper.address}}"{% endif %}{% if paper.author %},
    author="{{paper.author | join:' and ' }}"{% endif %}{% if paper.booktitle %},
    booktitle="{{paper.booktitle}}"{% endif %}{% if paper.date %},
    date="{{paper.date}}"{% endif %} {% if paper.description %},
    description="{{paper.description}}"{% endif %}{% if paper.doi %},
    doi="{{paper.doi}}"{% endif %} {% if paper.editor %},
    editor="{{paper.editor | join: ' and ' }}"{% endif %}{% if paper.isbn %},
    isbn="{{paper.isbn}}"{% endif %}{% if paper.journal %},
    journal="{{paper.journal}}"{% endif %}{% if paper.note %},
    note="{{paper.note}}"{% endif %}{% if paper.number %},
    number="{{paper.number}}"{% endif %}{% if paper.organization %},
    organization="{{paper.organization}}"{% endif %}{% if paper.pages %},
    pages="{{paper.pages}}"{% endif %}{% if paper.publisher %},
    publisher="{{paper.publisher}}"{% endif %}{% if paper.school %},
    school="{{paper.school}}"{% endif %}{% if paper.series %},
    series="{{paper.series}}"{% endif %}{% if paper.url %},
    url="{{paper.url}}"{% endif %}{% if paper.volume %},
    volume="{{paper.volume}}"{% endif %}{% if paper.year %},
    year="{{paper.year}}"
{% endif %}
}
{% endfor %}
